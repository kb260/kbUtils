<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kb260.kbutils">

    <application
        android:name=".app.KbApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.SlideActivity" />
        <activity android:name=".activityanim.ActivityAnimActivity" />
        <activity android:name=".activityanim.SecondActivity" />
        <activity android:name=".activityanim.ThirdActivity" />
        <activity android:name=".activityanim.FourActivity" />
        <activity android:name=".activityanim.FiveActivity" />
        <activity android:name=".view.SwipeActivity"></activity>
    </application>

</manifest>